{
    "version": 2,
    "builds": [
        {
            "src": "api/handler.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/handler.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "dist/public/assets/$1"
        },
        {
            "src": "/(favicon\\.ico|favicon\\.png)",
            "dest": "dist/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "dist/public/index.html"
        }
    ]
}